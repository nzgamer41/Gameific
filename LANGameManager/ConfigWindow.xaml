<Window x:Class="GameificClient.ConfigWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameificClient"
        mc:Ignorable="d"
        Title="ConfigWindow" Height="100" Width="300">
    <Grid>
        <Label Content="Game executable" HorizontalAlignment="Left" Margin="4,10,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="textBlockGamePath" Margin="10,36,0,0" TextWrapping="Wrap" Background="Gray" Text="TextBlock" VerticalAlignment="Top" Height="21" MouseDown="TextBlockGamePath_MouseDown" HorizontalAlignment="Left" Width="272"/>

    </Grid>
</Window>
