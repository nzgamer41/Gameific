        xmlns:local="clr-namespace:LANGameManager"
        mc:Ignorable="d"
        Title="InstallMenu" MinHeight="600" MinWidth="500" MaxHeight="600" MaxWidth="500"  Height="600" Width="500" Loaded="Window_Loaded">
