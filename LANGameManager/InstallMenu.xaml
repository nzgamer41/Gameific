        <Button Content="Cancel" HorizontalAlignment="Left" Margin="30,516,0,0" VerticalAlignment="Top" Width="188" Height="43" Click="Button_Click_1"/>
        <TextBlock x:Name="textBlockConsole" HorizontalAlignment="Left" Margin="10,382,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Background="#FFDCDADA" Height="100" Width="472" FontSize="14"/>
